{
  "schemaVersion": 1,
  "id": "mazeworld",
  "version": "${version}",

  "name": "MazeWorld",
  "description": "Generates a bedrock maze all the way to build height with normal world generation",
  "authors": [
    "replaceitem"
  ],
  "contact": {
    "homepage": "https://github.com/replaceitem",
    "sources": "https://github.com/replaceitem/mazeworld"
  },

  "license": "MIT",
  "icon": "assets/mazeworld/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.replaceitem.mazeworld.MazeWorld"
    ]
  },
  "mixins": [
    "mazeworld.mixins.json"
  ],
  "accessWidener": "mazeworld.accesswidener",

  "depends": {
    "minecraft": ">=1.21.5",
    "fabricloader": ">=0.15.10",
    "java": ">=21"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/etTDQAVSgt"
      }
